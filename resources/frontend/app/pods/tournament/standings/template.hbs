{{#with model as |tournament|}}

  <div class="row">
    {{#if tournament.isDraft }}
      {{#if session.isAuthenticated}}
        {{ggf-tournament.complete-setup}}
      {{/if}}
    {{else}}
      <div class="col s12">

        {{ggf-tournament.standings standings=tournament.standings}}

        {{ggf-tournament.tablescore tablescore=tournament.tablescore}}

      </div>
    {{/if}}
  </div>

  {{outlet}}

{{/with}}
