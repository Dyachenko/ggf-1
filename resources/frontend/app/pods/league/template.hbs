{{#with model as |league|}}
  <div class="row">
    <div class="col m12">
      <h3>{{league.name}}</h3>

      {{#md-tabs selected='teams'}}

        {{ggf-md.md-tab value='teams' title='Teams' link='league.teams'}}

      {{/md-tabs}}
      {{#if session.isAuthenticated }}

        {{#md-fixed-btns
          class="fixed-btns-example"
          btnIcon="mdi-content-add"
          btnClass="btn-large indigo"
          action="createTeam"
          actionArgs="0"}}

        {{/md-fixed-btns}}
      {{/if}}

      {{outlet}}
    </div>
  </div>
{{/with}}
