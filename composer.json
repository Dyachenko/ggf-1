{
    "name": "ggg/ggf",
    "version": "0.9.10",
    "description": "Good Gateway Football",
    "keywords": ["football", "fifa"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~5.5",
        "laravel/framework": "5.1.*",
        "laravel/homestead": "~2.1",
        "illuminate/html": "~5.0",
        "sorskod/larasponse": "dev-L5",
        "doctrine/dbal": "^2.5",
        "facebook/php-sdk-v4" : "^4.0.23",
        "barryvdh/laravel-ide-helper": "^2.0@dev",
        "guzzlehttp/guzzle": "^6.0@dev",
        "barryvdh/laravel-cors": "^0.7.0@dev",
        "laracasts/testdummy": "dev-master",
        "barryvdh/laravel-debugbar": "^2.1@dev",
        "intouch/laravel-newrelic": "~2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "setup": "./bin/setup.sh",
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "./bin/post-install.sh"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
